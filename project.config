[access]
	inheritFrom = All-Projects
[receive]
	requireChangeId = true
[project]
	description = OpenDaylight toolkit
[access "refs/*"]
	owner = group ldap/gerrit-toolkit-committers
	read = group Anonymous Users
	read = group Registered Users
	forgeAuthor = group ldap/gerrit-toolkit-committers
[access "refs/for/refs/*"]
	push = group Registered Users
	push = group ldap/gerrit-toolkit-committers
[access "refs/heads/*"]
	create = group Non-Interactive Toolkit Users
	forgeAuthor = group Non-Interactive Toolkit Users
	forgeCommitter = group Non-Interactive Toolkit Users
	push = group Non-Interactive Toolkit Users
	pushMerge = group Non-Interactive Toolkit Users
	label-Code-Review = -2..+2 group ldap/gerrit-toolkit-committers
	label-Code-Review = -1..+1 group Non-Interactive Toolkit Users
	label-Code-Review = -1..+1 group Registered Users
	label-Verified = -1..+1 group Non-Interactive Toolkit Users
	label-Verified = -1..+1 group ldap/gerrit-toolkit-committers
	submit = group ldap/gerrit-toolkit-committers
[access "refs/tags/*"]
	create = group ldap/gerrit-toolkit-committers
	forgeCommitter = group Non-Interactive Toolkit Users
	push = group Non-Interactive Toolkit Users
	push = +force group ldap/gerrit-toolkit-committers
	pushTag = group Non-Interactive Toolkit Users
	pushTag = +force group ldap/gerrit-toolkit-committers
